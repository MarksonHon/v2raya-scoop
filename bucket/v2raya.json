{
    "version": "1.5.7",
    "description": "v2rayA is a V2Ray client supporting global transparent proxy(Linux Only), compatible with SS, SSR, Trojan(trojan-go), PingTunnel protocols.",
    "homepage": "https://github.com/v2rayA/v2raya",
    "license": {
        "identifier": "AGPL-3.0-only",
        "url": "https://github.com/v2rayA/v2rayA/blob/master/LICENSE"
    },
    "depends": "v2ray",
    "suggest": {
        "v2ray-rules-dat": "v2raya/v2ray-rules-dat"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/v2rayA/v2rayA/releases/download/v1.5.7/v2raya_windows_x64_1.5.7.exe#/v2rayaWin.exe",
            "hash": "9c38da0a3f8f49d0c5e72f3e284fa33f04e6de5cbc09f32a1c27eb0a966f7487"
        }
    },
    "pre_install": [
        "info '[Portable Mode]: Copying user data...'",
        "if (Test-Path \"$persist_dir\\User Data\\*\") {",
        "    ensure \"$env:UserProfile\\.config\\v2raya\" | Out-Null",
        "    Copy-Item \"$persist_dir\\User Data\\*\" \"$env:UserProfile\\.config\\v2raya\\\" -Recurse -Force | Out-Null",
        "}",
        "Set-Content \"$dir\\v2raya.ps1\" -Value 'Start-Process -FilePath \"$env:SCOOP\\apps\\v2raya\\current\\v2rayaWin.exe\" -WorkingDirectory \"$env:SCOOP\\apps\\v2raya\\current\\\" -WindowStyle Hidden' -Encoding Ascii"
    ],
    "uninstaller": {
        "script": [
            "info '[Portable Mode]: Backing up user data...'",
            "if (Test-Path \"$env:UserProfile\\.config\\v2raya\\*\") {",
            "    ensure \"$persist_dir\" | Out-Null",
            "    Copy-Item \"$env:UserProfile\\.config\\v2raya\\*\" \"$persist_dir\\User Data\\\" -Recurse -Force | Out-Null",
            "}",
            "#Remove-Item \"$env:UserProfile\\.config\\v2raya\" -ErrorAction 'SilentlyContinue' -Force -Recurse"
        ]
    },
    "bin": [
        [
            "v2rayaWin.exe",
            "v2raya",
            "--lite"
        ],
        [
            "v2rayaWin.exe",
            "v2rayaWin"
        ]
    ],
    "env_set": {
        "V2RAYA_V2RAY_BIN": "$env:SCOOP\\apps\\v2ray\\current\\v2ray.exe"
    }
}